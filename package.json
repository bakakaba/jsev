{
  "name": "jsev",
  "version": "0.2.0",
  "description": "Base environment for building javascript applications.",
  "main": "src/index.js",
  "scripts": {
    "clean-output": "rm -rf .nyc_output && rm -rf output",
    "clean": "rm -rf node_modules && rm package-lock.json",
    "test": "eslint src && nyc node test",
    "fix": "eslint --fix src",
    "start": "NODE_ENV=local nodemon app.js"
  },
  "config": {
    "output_path": "output"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cruz82/jsev.git"
  },
  "keywords": [
    "environment",
    "utilites"
  ],
  "author": "https://github.com/cruz82",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cruz82/jsev/issues"
  },
  "homepage": "https://github.com/cruz82/jsev#readme",
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "app-module-path": "^2.2.0",
    "bunyan": "^1.8.12",
    "bunyan-sumologic": "^1.2.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "graphql": "^0.11.0",
    "koa": "^2.4.1",
    "koa-graphql": "^0.7.3",
    "koa-jwt": "^3.3.0",
    "koa-router": "^7.3.0",
    "nodemon": "^1.14.3",
    "raygun": "^0.9.1",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "jsev-dev": "0.0.9"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./output/coverage"
  }
}
