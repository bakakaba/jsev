{
  "name": "jsev",
  "version": "0.8.2",
  "description": "Environment for building Web API's.",
  "main": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean-output": "rm -rf .nyc_output && rm -rf output",
    "clean": "rm -rf node_modules && rm package-lock.json",
    "test": "eslint src && nyc node test",
    "fix": "eslint --fix src",
    "start": "NODE_ENV=local nodemon app.js",
    "build": "tsc"
  },
  "config": {
    "output_path": "output"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cruz82/jsev.git"
  },
  "keywords": [
    "environment",
    "utilites"
  ],
  "author": "https://github.com/cruz82",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cruz82/jsev/issues"
  },
  "homepage": "https://github.com/cruz82/jsev#readme",
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "app-module-path": "^2.2.0",
    "bunyan": "^1.8.12",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "change-case": "^3.1.0",
    "graphql": "^14.2.1",
    "koa": "^2.7.0",
    "koa-graphql": "^0.8.0",
    "koa-jwt": "^3.5.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "nodemon": "^1.18.11",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/node": "^10.15.0",
    "typescript": "^3.3.4000"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./output/coverage"
  }
}
