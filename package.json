{
  "name": "jsev",
  "version": "0.8.2",
  "description": "Environment for building Web API's.",
  "main": "lib/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm package-lock.json && rm -rf lib",
    "test": "eslint src && nyc node test",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write src/**/*",
    "start": "tsc -w",
    "build": "tsc"
  },
  "config": {
    "output_path": "output"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cruz82/jsev.git"
  },
  "keywords": [
    "environment",
    "utilites"
  ],
  "author": "https://github.com/cruz82",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cruz82/jsev/issues"
  },
  "homepage": "https://github.com/cruz82/jsev#readme",
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "apollo-server-koa": "^2.4.8",
    "bunyan": "^1.8.12",
    "callsites": "^3.1.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "change-case": "^3.1.0",
    "graphql": "^14.2.1",
    "koa": "^2.7.0",
    "koa-jwt": "^3.5.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "shortid": "^2.2.14",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/chalk": "^2.2.0",
    "@types/graphql": "^14.2.0",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/koa__cors": "^2.2.3",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.14.5",
    "@types/shortid": "0.0.29",
    "@types/source-map-support": "^0.5.0",
    "app-module-path": "^2.2.0",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./output/coverage"
  },
  "files": ["lib/**/*"]
}
